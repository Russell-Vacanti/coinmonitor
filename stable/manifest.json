{
  "manifest_version": 2,

  "name": "Coin Monitor",
  "description": "Track the prices of cryptocurrency quickly",
  "version": "1.0",
  "icons" :
  {
    "128" : "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "https://min-api.cryptocompare.com"
  ]
}