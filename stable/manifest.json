{
  "manifest_version": 2,

  "name": "Coin Monitor",
  "description": "Track the prices of cryptocurrency quickly",
  "version": "1.9",
  "icons" :
  {
    "128" : "./lib/icons/icon_new.png"
  },
  "browser_action": {
    "default_icon": "./lib/icons/icon_new.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "https://min-api.cryptocompare.com"
  ]
}
